          return "tortoiseshell or dilute tortoiseshell";
        }
      } else if (base1 === "blackchoc" || base2 === "blackchoc") {
        if (dilute1 === "nodilute" || dilute2 === "nodilute") {
          return "tortoiseshell or chocolate tortoiseshell";
        } else if (dilute1 === "dilute" && dilute2 === "dilute") {
          return "dilute tortoiseshell or lilac-cream tortoiseshell";
        } else {
          return "tortoiseshell, chocolate tortie, dilute tortie, or lilac-cream tortie";
        }
      } else if (base1 === "blackcinn" || base2 === "blackcinn") {
        if (base1 === "chocolate" || base2 === "chocolate") {
          if (dilute1 === "nodilute" || dilute2 === "nodilute") {
            return "tortoiseshell or chocolate tortoiseshell";
          } else if (dilute1 === "dilute" && dilute2 === "dilute") {
            return "dilute tortoiseshell or lilac-cream tortoiseshell";
          } else {
            return "tortoiseshell, chocolate tortie, dilute tortie, or lilac-cream tortie";
          }
        } else if (base1 === "choccinn" || base2 === "choccinn") {
          if (dilute1 === "nodilute" || dilute2 === "nodilute") {
            return "tortoiseshell, chocolate tortie, or cinnamon tortie";
          } else if (dilute1 === "dilute" && dilute2 === "dilute") {
            return "dilute tortoiseshell, lilac-cream tortie, or fawn-cream tortie";
          } else {
            return "tortoiseshell, chocolate tortie, cinnamon tortie, dilute tortie, lilac-cream tortie, or fawn-cream tortie";
          }
        } else {
          if (dilute1 === "nodilute" || dilute2 === "nodilute") {
            return "tortoiseshell or cinnamon tortoiseshell";
          } else if (dilute1 === "dilute" && dilute2 === "dilute") {
            return "dilute tortoiseshell or fawn-cream tortoiseshell";
          } else {
            return "tortoiseshell, cinnamon tortie, dilute tortie, or fawn-cream tortie";
          }
        }

      } else if (base1 === "chocolate" || base2 === "chocolate") {
        if (dilute1 === "nodilute" || dilute2 === "nodilute") {
          return "chocolate tortoiseshell";
        } else if (dilute1 === "dilute" && dilute2 === "dilute") {
          return "lilac-cream tortoiseshell";
        } else {
          return "chocolate tortoiseshell or lilac-cream tortoiseshell";
        }
      } else if (base1 === "choccinn" || base2 === "choccinn") {
        if (dilute1 === "nodilute" || dilute2 === "nodilute") {
          return "chocolate tortoiseshell or cinnamon tortoiseshell";
        } else if (dilute1 === "dilute" && dilute2 === "dilute") {
          return "lilac-cream tortoiseshell or fawn-cream tortoiseshell";
        } else {
          return "chocolate tortoisheshell, cinnamon tortie, lilac-cream tortie, or fawn-cream tortie";
        }
      } else {
        if (dilute1 === "nodilute" || dilute2 === "nodilute") {
          return "cinnamon tortoiseshell";
        } else if (dilute1 === "dilute" && dilute2 === "dilute") {
          return "fawn-cream tortoiseshell";
        } else {
          return "cinnamon tortoiseshell or fawn-cream tortoiseshell";
        }
      }
    } 
  }

  function tabby() {
  	var agouti1 = document.getElementById("agouti1").value;
    var agouti2 = document.getElementById("agouti2").value;
    var tabby1 = document.getElementById("tabby1").value;
    var tabby2 = document.getElementById("tabby2").value;
    var text = "";
    if (tabby1 === "ticked" || tabby2 === "ticked"){
    	if (agouti1 === "solid" && agouti2 === "solid"){
      	return "All kittens will be genetically solid, but red kittens will display ticked tabby, spotted tabby, mackerel tabby, or classic tabby.";
      }else if (agouti1 === "tabby" || agouti2 === "tabby"){
      	return "Kittens can be ticked tabby, spotted tabby, mackerel tabby, or classic tabby.";
      }else{
      	return "Kittens can be solid, ticked tabby, spotted tabby, mackerel tabby, or classic tabby. Genetically solid reds still display a tabby pattern.";
      }   
    }else if (tabby1 === "spotted" || tabby2 === "spotted"){
    	if (agouti1 === "solid" && agouti2 === "solid"){
      	return "All kittens will be genetically solid, but red kittens will display spotted tabby, mackerel tabby, or classic tabby.";
      }else if (agouti1 === "tabby" || agouti2 === "tabby"){
      	return "Kittens can be spotted tabby, mackerel tabby, or classic tabby.";
      }else{
      	return "Kittens can be solid, spotted tabby, mackerel tabby, or classic tabby. Genetically solid reds still display a tabby pattern.";
      }
    }else if (tabby1 === "mackerel" || tabby2 === "mackerel"){
    	if (agouti1 === "solid" && agouti2 === "solid"){
      	return "All kittens will be genetically solid, but red kittens will display mackerel tabby or classic tabby.";
      }else if (agouti1 === "tabby" || agouti2 === "tabby"){
      	return "Kittens can be mackerel tabby or classic tabby.";
      }else{
      	return "Kittens can be solid, mackerel tabby, or classic tabby. Genetically solid reds still display a tabby pattern.";
      }
    }else{
    	if (agouti1 === "solid" && agouti2 === "solid"){
      	return "All kittens will be genetically solid, but red kittens will display classic tabby.";
      }else if (agouti1 === "tabby" || agouti2 === "tabby"){
      	return "Kittens will all be classic tabby.";
      }else{
      	return "Kittens can be solid or classic tabby. Genetically solid reds still display a tabby pattern.";
      }
    }
  }

  function silver() {
    var silver1 = document.getElementById("silver1").value;
    var silver2 = document.getElementById("silver2").value;
    var text = "";
    if (silver1 === "nosilver" && silver2 === 'nosilver') {
      return '';
    } else if (silver1 === "silver" || silver2 === 'silver') {
      return ' All kittens will be silver/smoke.';
    } else {
      return ' Some kittens may be silver/smoke.';
    }
  }

  function colorpoint() {
    var point1 = document.getElementById("point1").value;
    var point2 = document.getElementById("point2").value;
    var text = "";
    if (point1 === "nopoint" || point2 === 'nopoint') {
      return '';
    } else if (point1 === "point" && point2 === 'point') {
      return ' All kittens will be colorpoints.';
    } else {
      return ' Some kittens may be colorpoints.';
    }
  }
  
	function domwhite() {
  	var white1 = document.getElementById("white1").value;
    var white2 = document.getElementById("white2").value;
    var text = "";
  	if (white1 === "domwhite" || white2 === "domwhite"){
    return "";
    }else if(white1 === "domwhitenone" || white2 === "domwhitenone" || white1 === "domwhitespot" || white2 === "domwhitespot"){
    return ", or dominant white";
    }else{
    return "";
    }
    
  }
  
  function white() {
    var white1 = document.getElementById("white1").value;
    var white2 = document.getElementById("white2").value;
    var text = "";
    if (white1 === "domwhite" || white2 === "domwhite"){
    	return "";
    } else if (white1 === "domwhitespot" || white2 === "domwhitespot"){
    	if (white1 === "domwhitespot" && white2 === "domwhitespot"){
      	return " Those that aren't dominant white will have over 50% white spotting.";
      } else if (white1 === "domwhitenone" || white2 === "domwhitenone"){
      	return " Those that aren't dominant white will have under 50% white spotting.";
      } else if (white1 === "bigwhite" || white2 === "bigwhite"){
      	return " Those that aren't dominant white will have over 50% white spotting.";
      } else if (white1 === "smallwhite" || white2 === "smallwhite"){
      	return " Those that aren't dominant white can have any amount of white spotting.";
      } else {
      	return " Those that aren't dominant white will have under 50% white spotting.";
      }
    } else if (white1 === "domwhitenone" || white2 === "domwhitenone"){
      if (white1 === "domwhitenone" && white2 === "domwhitenone"){
      	return " Those that aren't dominant white will have no white spotting.";
      } else if (white1 === "bigwhite" || white2 === "bigwhite"){
      	return " Those that aren't dominant white will have under 50% white spotting.";
      } else if (white1 === "smallwhite" || white2 === "smallwhite"){
      	return " Those that aren't dominant white can have under 50% white spotting, or none at all.";
      } else {
      	return " Those that aren't dominant white will have no white spotting.";
      }
    } else if (white1 === "bigwhite" || white2 === "bigwhite"){
    	if (white1 === "bigwhite" && white2 === "bigwhite"){
      	return " All kittens will have over 50% white spotting.";
      } else if (white1 === "smallwhite" || white2 === "smallwhite"){
      	return " Kittens can have any amount of white spotting.";
      } else {
      	return " All kittens will have under 50% white spotting.";
      }
    } else if (white1 === "smallwhite" || white2 === "smallwhite"){
      if (white1 === "smallwhite" && white2 === "smallwhite"){
      	return " Kittens can have any amount of white spotting or lack thereof.";
      } else {
      	return " Kittens can have under 50% white spotting or none at all.";
      }
    } else {
    	return " Kittens will have no white spotting."
    }
  }
  
  $('.general').text("Kittens " + furLength() + ". Toms can be " + coatColorTom() + domwhite() + ". Mollies can be " + coatColorMolly() + domwhite() + ". " + tabby() + silver() + colorpoint() + white());
}
</script>
